body {
	margin:0 auto;
	font-family:"黑体";
	font-family:"微软雅黑";
}
a {
	text-decoration: none;
	
}
.header {
	margin:0 auto;
	width:100%;
	height:128px;
	background:url(../images/header_bg.png);
}
		
.header_up {
	margin:0 auto;
	width:1000px;
	height:73px;
}
.header_down {
	margin:0 auto;
	width:1000px;
	height:54px;
}
.logo,.menu {
	float:left;
}
.search,.login {
	float:right;
}
.logo {
	/*padding-left:45px;*/
}
.search {
	padding-top:42px;
}
.search a,.login a {
	font-size:12px;
	color:#3d3d3d;
	font-family:"微软雅黑";  
 }

.menu {
	padding:0;
	margin:0;
}
.menu li {
	list-style:none; 
	float:left;
	margin-left:45px;
	padding-top:10px; 
	text-align:center;
}
.menu li a {
	font-size:18px;
	color:#3d3d3d;  
}

.menu li:hover a {
	color:#268bd9;
}

.login {
	padding-top:24px;	
}
.login img {
	width:12px;
	height:12px;
}
.footer {
	margin:0 auto;
	width:100%;
	height:90px;
	border-top:1px solid #c0c0c0; 
	background:#e2e2e2;	
}
.footer ul {
	margin:0 auto;
	padding:0;
	width:1000px;
}
.footer ul li {
	list-style:none; 
	float:left;
	width:82px;
	line-height:90px;
	text-align:left;
}
.footer ul li a {
	font-size:12px;
	color:#969696; 
	font-family:"微软雅黑"; 
}
.footer ul li a:hover{
	color:#268bd9;
}
.congnitive_w {
background:url(../img/r_bg.gif);
width:100%;
height:40px;}
